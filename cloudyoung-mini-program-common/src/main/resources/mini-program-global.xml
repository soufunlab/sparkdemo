<?xml version="1.0" encoding="UTF-8"?>

<global>
    <hbase>
        <hbase.zookeeper.quorum>master</hbase.zookeeper.quorum>
        <hbase.zookeeper.property.clientPort>2181</hbase.zookeeper.property.clientPort>
        <zookeeper.znode.parent>/hbase</zookeeper.znode.parent>
    </hbase>
    <kafka>
        <producer>
            <bootstrap.servers>master:9092</bootstrap.servers>
            <topic>love_car</topic>
            <acks>all</acks>
            <retries>0</retries>
            <batch.size>16384</batch.size>
            <buffer.memory>33554432</buffer.memory>
            <key.serializer>org.apache.kafka.common.serialization.StringSerializer</key.serializer>
            <value.serializer>org.apache.kafka.common.serialization.StringSerializer</value.serializer>
        </producer>
        <consumer>
            <bootstrap.servers>master:9092</bootstrap.servers>
            <topic>cloud-young-kafka-datamock</topic>
            <group.id>testtest</group.id>
            <enable.auto.commit>true</enable.auto.commit>
            <auto.offset.reset>earliest</auto.offset.reset>
            <auto.commit.interval.ms>1000</auto.commit.interval.ms>
            <session.timeout.ms>30000</session.timeout.ms>
            <key.deserializer>org.apache.kafka.common.serialization.StringDeserializer</key.deserializer>
            <value.deserializer>org.apache.kafka.common.serialization.StringDeserializer</value.deserializer>
        </consumer>
    </kafka>

    <mysql>
        <mini-program>
            <jdbc.driver>com.mysql.jdbc.Driver</jdbc.driver>
            <jdbc.url>jdbc:mysql://192.168.10.21/bigdata_center?useUnicode=true&amp;characterEncoding=utf8&amp;autoReconnect=true&amp;zeroDateTimeBehavior=convertToNull&amp;allowMultiQueries=true</jdbc.url>
            <jdbc.username>bigdata_admin</jdbc.username>
            <jdbc.password>bigdata_admin@dev.com</jdbc.password>
        </mini-program>
    </mysql>

    <elasticsearch>
        <mini-program>
            <cluster>master:9200,slave1:9200</cluster>
        </mini-program>
    </elasticsearch>

    <ip-database>
        <global-world>G:/yunyang/ip库/default.txt</global-world>
        <global-china>G:/yunyang/ip库/succ.txt</global-china>
    </ip-database>

    <etl-conf>
        <process-thead-num>1</process-thead-num>
        <hbase-buffer-size-byte>10485760</hbase-buffer-size-byte>
        <auto-flush-hbase-process>true</auto-flush-hbase-process>
        <auto-flush-hbase-interval-time-ms>30000</auto-flush-hbase-interval-time-ms>
        <flush-elasticsearch-size>50</flush-elasticsearch-size>
        <auto-flush-elasticsearch-process>true</auto-flush-elasticsearch-process>
        <auto-flush-elasticsearch-interval-time-ms>30000</auto-flush-elasticsearch-interval-time-ms>
    </etl-conf>

</global>